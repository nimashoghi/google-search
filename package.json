{
    "name": "google-search",
    "displayName": "Google Search",
    "description": "VSC Google search feature",
    "version": "0.0.1",
    "publisher": "nimashoghi",
    "engines": {
        "vscode": "^1.31.0"
    },
    "author": {
        "name": "Nima Shoghi",
        "url": "https://www.github.com/nimashoghi"
    },
    "icon": "resources/icon.png",
    "homepage": "https://www.github.com/nimashoghi/google-search",
    "repository": {
        "type": "git",
        "url": "https://www.github.com/nimashoghi/google-search"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "dist/index.js",
    "contributes": {
        "menus": {
            "editor/context": [
                {
                    "command": "google-search.search",
                    "group": "navigation"
                }
            ],
            "editor/title": [
                {
                    "when": "editorTextFocus",
                    "command": "google-search.search",
                    "group": "navigation"
                }
            ]
        },
        "commands": [
            {
                "icon": "resources/icon.png",
                "command": "google-search.search",
                "title": "Google Search..."
            }
        ],
        "configuration": {
            "title": "Google Search Config",
            "type": "object",
            "properties": {
                "google-search.template": {
                    "type": "string",
                    "default": "https://www.google.com/search?q=%QUERY%",
                    "description": "Search Query"
                }
            }
        }
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "build": "tsc -b .",
        "watch": "tsc -b --watch ."
    },
    "devDependencies": {
        "@types/mocha": "^5.2.7",
        "@types/node": "^12.6.2",
        "@typescript-eslint/eslint-plugin": "^1.12.0",
        "@typescript-eslint/parser": "^1.12.0",
        "eslint": "^6.0.1",
        "eslint-config-prettier": "^6.0.0",
        "eslint-plugin-prettier": "^3.1.0",
        "prettier": "^1.18.2",
        "typescript": "^3.5.3",
        "vscode": "^1.1.35"
    }
}
